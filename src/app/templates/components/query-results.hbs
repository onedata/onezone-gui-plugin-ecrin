<div class="data-start-row" style={{firstRowStyle}}></div>
{{#bs-accordion as |acc|}}
  {{#each results as |result|}}
    <div class="data-row" data-row-id={{result.id}}>
      {{#acc.item value=result._id as |accItem|}}
        {{query-results/result item=accItem result=result}}
      {{/acc.item}}
    </div>
  {{/each}}
{{/bs-accordion}}
{{loading-container
  isLoading=bottomLoading
  errorReason=results.error
  sizeClass="sm"
  spinnerBlockClass="horizontal-align-middle"}}
{{yield}}
